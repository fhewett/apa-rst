	<body>
		<segment id="1" >Ein weißer Orang-Utan ist wieder zurück in der Natur</segment>
		<segment id="2" parent="13" relname="span">Auf der Insel Borneo in Asien lebt im Dschungel eine Affenart namens Orang-Utan.</segment>
		<segment id="3" parent="2" relname="e-elaboration">Diese Menschenaffen haben normalerweise rot-braunes Fell.</segment>
		<segment id="4" parent="14" relname="span">Im April 2017 wurde aber in einem Dorf ein weißes Orang-Utan Weibchen gefunden.</segment>
		<segment id="5" parent="4" relname="e-elaboration">Es war verletzt.</segment>
		<segment id="6" parent="15" relname="sequence">Das Orang-Utan Weibchen wurde von Pflegern gesund gepflegt</segment>
		<segment id="7" parent="15" relname="sequence">und Alba getauft.</segment>
		<segment id="8" parent="24" relname="span">Jetzt wurde Alba zurück in den Dschungel gebracht.</segment>
		<segment id="9" parent="10" relname="cause">Weiße Orang-Utans sind sehr selten.</segment>
		<segment id="10" parent="16" relname="span">Daher kann es sein, dass Wilderer Alba jagen werden.</segment>
		<segment id="11" parent="22" relname="span">Deshalb steht Alba unter besonderem Schutz.</segment>
		<segment id="12" parent="11" relname="elaboration">Sie wird Tag und Nacht von insgesamt 24 Personen bewacht.</segment>
